{
    "name": "mdjaman/mdjaman-common",
    "description": "Module that provides common functionality and library code for ZF2 applications",
    "type": "library",
    "keywords": [
        "zf2",
        "zend",
        "module",
        "common",
        "abstract"
    ],
    "authors": [
        {
            "name": "Marcel Djaman",
            "email": "marceldjaman@gmail.com",
            "homepage": "http://github.com/mdjaman/"
        }
    ],
    "homepage": "https://github.com/mdjaman/MdjamanCommon",
    "license": "MIT",
    "minimum-stability": "dev",
    "require": {
        "php": ">=5.4",
        "zendframework/zendframework": "2.3.*",
        "doctrine/doctrine-orm-module": "0.*",
        "gedmo/doctrine-extensions": "dev-master",
        "sp/jms-serializer-module": "dev-master"
    },
    "require-dev": {
        "phpunit/phpunit": "4.8.*",
        "zendframework/zftool": "dev-master"
    },
    "suggest": {
        "doctrine/doctrine-mongo-odm-module": "If you want to use Doctrine ODM",
        "imagine/imagine" : "If you need image manipulation after uploading image"
    },
    "autoload": {
        "psr-0": {
            "MdjamanCommon": "src/"
        },
        "classmap": [
            "./Module.php"
        ]
    }
}
